.container {
  background-color: #fff;
  min-height: 100%;
  display: block;
}
.category-box {  
  background-color: #fff;
  clear: both;
}
.category-list{
  width:150rpx;
  margin-top:20rpx;
  text-align: center;
  display: inline-block;
  overflow: hidden;
}
.category-column{
  width:100%;
  margin-top:20rpx;
  overflow: hidden;
}
.category-imgbox{
  width:60rpx;
  height:60rpx;
}
.category-title{
  font-size: 24rpx;
  text-align:center;
  margin-top:10rpx;
}


.swiper-container{
  width: 750rpx;
  position: relative;  
}
.swiper_box {
  width: 750rpx;
  height:300rpx;
}

swiper-item image {
  width: 750rpx;
  height:300rpx;
}
.swiper-container .dots{  
  position: absolute;  
  left: 0;  
  right: 0;  
  bottom: 20rpx;  
  display: flex;  
  justify-content: center;  
}  
.swiper-container .dots .dot{  
  margin: 0 8rpx;  
  width: 14rpx;  
  height: 14rpx;  
  background: #fff;  
  border-radius: 50%;  
  transition: all .6s;  
  opacity: .5;
}  
.swiper-container .dots .dot.active{  
  width: 14rpx;  
  opacity: 1;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.type-item-on{
  color:#e64340;
  border-bottom: 1rpx solid #e64340;
}
.weui-loadmore {
  margin-bottom: 0px;
}
.goods-container{
  display: flex;
  justify-content: space-between;  
  flex-wrap:wrap;
  box-sizing: content-box;
  
  background-color: #F2f2f2;
}
.goods-box{

  background-color: #fff;
  overflow: hidden;

  border-radius:5px;

  padding:0;
  margin:0;
  box-sizing:border-box;
  flex:0 0 50%;
  padding-right:5rpx;
  border-bottom:2rpx solid #e5e5e5;
  border-right:2rpx solid #e5e5e5;
  align-content:start;
  position:relative;
  height:auto;
  width:auto;
  display:block;



}
.goods-box .img-box{
  width: 100%;
  height: 369rpx;
  overflow: hidden;

  
}
.goods-box .img-box image{
   width: 100%;
   height: 369rpx;
}
.goods-box .img-box image2{
   width: 100%;
   height: 369rpx;
   position: absolute;
   overflow: visible
}
.outofstock {
  width: 100%;
  height: 100%;  
  position: absolute;
  overflow: hidden;
  top: 0rpx;
  right: 2rpx;
  margin: auto;
  text-align: center;
}  

.goods-box .goods-title{
  width: 280rpx;
  text-overflow: ellipsis;

  overflow: hidden;

  padding: 24rpx 0 0rpx 0;
  color:#000;
  margin-left: 24rpx;

  font-size:28rpx;
  font-weight:300;
  word-break:normal;

}
.goods-box .goods-title5{
  width: 280rpx;
  text-overflow: ellipsis;

  overflow: hidden;

  padding: 24rpx 0 0rpx 0;
  color:#000;
  margin-left: 24rpx;

  font-size:25rpx;
  font-weight:300;
  word-break:normal;

}
.goods-box .goods-price{
  width: 110rpx;
  overflow: hidden;

  padding: 24rpx 0;
  color:#e64340;
  margin-left: 24rpx;

  font-size:32rpx;
  font-weight:300;

}
.no-more-goods{
  text-align: center;
  font-size: 24rpx;
  padding-bottom:48rpx; 
  color: #999;
}
.notice{
  display: flex;
  justify-content: space-between;  
  flex-wrap:wrap;
  height: 80rpx;
  line-height: 80rpx;
  width: 750rpx;
  margin-top: 10rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #efeff4;
}
.notice_icon {
  margin-top: 25rpx;
  margin-left:20rpx;
  width:30rpx;
  height:30rpx;
}
.notice_swiper{
  height: 80rpx;
  line-height: 80rpx;
